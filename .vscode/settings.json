{
  /*Cấu hình theme mặc định*/
  "workbench.colorTheme": "Default Dark+",

  /*Hộp thoại xác nhận xóa file, folder*/
  "explorer.confirmDelete": false,

  /*Tự động lưu*/
  "files.autoSave": "onFocusChange",

  /*Config Formatter code*/
  "editor.formatOnSave": true,
  "editor.tabSize": 2,

  "prettier.semi": false,
  "prettier.jsxSingleQuote": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.printWidth": 200,
  "prettier.proseWrap": "always",
  "prettier.requireConfig": true,
  "blade.format.enable": true,
  "bladeFormatter.format.indentSize": 2,
  "scssFormatter.singleQuote": true,
  "typescript.preferences.quoteStyle": "single",
  "eslint.format.enable": true,

  /*defaultFormatter*/
  "editor.defaultFormatter": "esbenp.prettier-vscode", //Default: esbenp.prettier-vscode
  "[blade]": { "editor.defaultFormatter": "shufo.vscode-blade-formatter" },
  "[php]": { "editor.defaultFormatter": "bmewburn.vscode-intelephense-client" },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[typescript]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
  "[typescriptreact]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
  "[javascript]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
  "[javascriptreact]": { "editor.defaultFormatter": "vscode.typescript-language-features" },

  /*Cấu hình compiler scss
   * https://github.com/ritwickdey/vscode-live-sass-compiler/blob/master/docs/settings.md
   */
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded" /*expanded, compressed */,
      "extensionName": ".min.css",
      "savePath": "~/../"
    }
  ],

  "liveSassCompile.settings.generateMap": true,
  /*Những mục không compiler*/
  "liveSassCompile.settings.excludeList": [
    /*Environment*/
    "**/.vscode/**",
    "**/.data/**",
    /*Laravel*/
    "**/app/**",
    "**/bootstrap/**",
    "**/config/**",
    "**/database/**",
    "**/lang/**",
    "**/routes/**",
    "**/resources/**",
    "**/storage/**",
    "**/tests/**",
    "**/vendor/**",
    /*Plugins*/
    "**/node_modules/**",
    "**/plugins/**",
    "**/bootstrap/**",
    "**/vendor/**",
    "**/fontawesome/**"
  ],

  "explorer.confirmDragAndDrop": false,
  "editor.fontSize": 16,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.largeFileOptimizations": false,
  "editor.linkedEditing": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.autofetch": true,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.maxTokenizationLineLength": 2000,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "es6-css-minify.genCSSmap": null,
  "es6-css-minify.genJSmap": true,
  "es6-css-minify.enableAutoprefixerSelection": true,
  "es6-css-minify.enableAutoprefixer": true,
  "diffEditor.maxFileSize": 1000,
  "editor.detectIndentation": false,
  "intelephense.completion.maxItems": 1000,
  "bladeFormatter.format.wrapLineLength": 1000,
  "scssFormatter.printWidth": 1000,
  "git.ignoreLimitWarning": true,
  "tslint.packageManager": "yarn",
  "tslint.jsEnable": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsserver.enableTracing": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": true,
  "intelephense.diagnostics.undefinedMethods": false,
  "terminal.integrated.enableMultiLinePasteWarning": false
}